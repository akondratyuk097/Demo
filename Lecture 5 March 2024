################## library for mean 
cpp file>
#include "average.h"




double average(int x[]) {
    int sumx = 0;
    int len = sizeof(x) / sizeof(x[0]);

    for (int i = 0; i < len; i++) {
        sumx += x[i];
    }

    return (double)sumx / len;
}
.h file >
#ifndef average_h
#define average_h
#include <Arduino.h>

double average (int x[]);


#endif









